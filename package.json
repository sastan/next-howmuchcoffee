{
  "name": "next-howmuchcoffee",
  "version": "0.1.0",
  "description": "Next.js starter styled with Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "analyze:tailwind-css-class-order": "rustywind --dry-run .",
    "analyze": "yarn run analyze:tailwind-css-class-order",
    "fix:tailwind-css-class-order": "rustywind --write .",
    "fix": "yarn run fix:tailwind-css-class-order && echo && yarn run format",
    "dev": "next",
    "build": "next build",
    "export": "yarn run build && next export",
    "start": "next start",
    "format": "prettier-standard --format",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozart409/next-howmuchcoffee.git"
  },
  "author": "Amadeus Mader",
  "license": "MIT",
  "homepage": "https://howmuchcoffee.mozart409.space",
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "next": "10.0.3",
    "next-themes": "^0.0.10",
    "postcss": "^8.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "^17.0.1",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "prettier-standard": "^16.4.1",
    "rustywind": "0.7.0",
    "tailwindcss": "2.0.2"
  }
}
